{
    "data": {
        "filename": "data/bitcoin2.csv",
        "filename_clean": "data/clean_data.h5",
        "filter_columns": [
            "Open",
            "Close",
            "Volume_(BTC)",
            "Volume_(Currency)",
            "Month"
        ],
        "batch_size": 250,
        "train_test_split": 0.8,
        "x_window_size": 150,
        "y_window_size": 1,
        "y_predict_column": "Close"
    },
    "model": {
        "epochs": 100,
        "loss_function": "mse",
        "optimiser_function": "adam",
        "filename_model": "data/model_saved.h5",
        "filename_predictions": "data/model_predictions.h5"
    }
}